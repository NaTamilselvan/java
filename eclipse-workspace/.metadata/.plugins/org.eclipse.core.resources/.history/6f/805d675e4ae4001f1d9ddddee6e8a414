package StarPattern;

public class NumberPattern {

	public static void main(String[] args) {
		
		int k=1;
		
		for( int i=1;i<=5;i++ ) {
			
			for( int j=1;j<=5;j++ ){
				 
				if (i%2!=0) {
				 System.out.print( k );
				 k++;
				    }
				else
				 System.out.print(i);
			}
			
			System.out.println();
		}
		
		

	}

}
