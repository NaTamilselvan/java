package l1;

public class SecoundLargeElement {
	
	
	public int secoundLarge(int[] arr) {
		
		//first i find max value;
		
		int max=0;
		
		
		for(int i=0;i<arr.length;i++) {
			
			if ( arr[i] > max) {
				
				max=arr[i];
				
				
				
			}
			
		}
		
		
		
		return max;
	}
	
	

	public static void main(String[] args) {
		
		
		//declare  Array

		
		//int[] a=new int[5];
		
		//Array declare and initialization in single line
		
		int[] a= {10,34,54,23,56,34};
		
		SecoundLargeElement s=new SecoundLargeElement();
		
		int result=s.secoundLarge(a);
		
		System.out.println(result);
		
		
		
	}

}
