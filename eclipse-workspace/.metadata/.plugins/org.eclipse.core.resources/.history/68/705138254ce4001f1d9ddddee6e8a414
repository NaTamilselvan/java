package StarPattern;

public class NumberPattern {
	
	void p1() {
		
        int k=1,l=0;
		
		for( int i=1;i<=5;i++ ) {
			
			for( int j=1;j<=5;j++ ){
				
				if (i%2==0) {
					k=k+5;
					System.out.print(k+" ");
					k--;
				}
			    
				else {
				System.out.print(k++ +" ");
				l=k;}
			}
			
			System.out.println();
		}
		
		
	}

	public static void main(String[] args) {
		
		
		for(int i=1;i<=5;i++){
			
			for(int j=1;j<=2;j++) {
				
		         System.out.print(i);		
			}
			
			System.out.println();
		}
		
	}

}
